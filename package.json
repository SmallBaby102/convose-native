  "scripts": {
    "build": "tsc --p tsconfig.json",
    "start": "react-native start",
    "lint": "eslint src --fix --ext .ts,.tsx ",
    "eject": "expo eject",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "precommit": "yarn build && yarn test && yarn lint",
    "devtools": "react-devtools"
  },
  "config": {},
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(.*expo.*|.*react.*)/)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
